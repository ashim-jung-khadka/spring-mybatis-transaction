<?xml version="1.0" encoding="UTF-8" ?>
<!DOCTYPE mapper PUBLIC "-//mybatis.org//DTD Mapper 3.0//EN" "http://mybatis.org/dtd/mybatis-3-mapper.dtd" >

<mapper namespace="com.ashim.springmybatis.mapper.PersonMapper">

	<select id="insert" parameterType="com.ashim.springmybatis.bean.Person">
		insert into person (name, address)
		values ( #{name}, #{address} )
	</select>

</mapper>